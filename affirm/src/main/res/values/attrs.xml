<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AffirmPromotionButton">
        <attr name="affirmLogoType" format="enum">
            <enum name="AffirmDisplayTypeLogo" value="0"/>
            <enum name="AffirmDisplayTypeText" value="1"/>
            <enum name="AffirmDisplayTypeSymbol" value="2"/>
            <enum name="AffirmDisplayTypeSymbolHollow" value="3"/>
        </attr>
        <attr name="affirmColor" format="enum">
            <enum name="AffirmColorTypeBlue" value="0"/>
            <enum name="AffirmColorTypeBlack" value="1"/>
            <enum name="AffirmColorTypeWhite" value="2"/>
        </attr>
        <attr name="htmlStyling" format="boolean"/>
        <attr name="affirmTextSize" format="dimension"/>
        <attr name="affirmTextColor" format="color"/>
        <attr name="affirmTextFont" format="reference"/>
    </declare-styleable>
</resources>